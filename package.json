{
  "name": "zg-frontend-2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "i18n:extract": "formatjs extract \"src/**/*.{ts,tsx}\" --out-file content/locales/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile-folder --ast content/locales content/compiled-locales",
    "i18n": "npm run i18n:extract && npm run i18n:compile",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.4",
    "@svgr/webpack": "^5.5.0",
    "@types/numeral": "^2.0.2",
    "class-transformer": "^0.4.0",
    "cross-env": "^7.0.3",
    "inspirational-quotes": "^2.0.1",
    "lodash": "^4.17.21",
    "next": "12.0.3",
    "numeral": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intl": "^5.21.2",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.4.0",
    "@types/lodash": "^4.14.177",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@types/sass": "^1.43.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "typescript": "4.4.4"
  }
}
